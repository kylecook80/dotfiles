#!/bin/bash

DIR="${BASH_SOURCE%/*}"
if [[ ! -d "$DIR" ]]; then DIR="$PWD"; fi
. "$DIR/lib/installer-util.sh"

SAFE=true
while getopts "u" o; do
    case ${o} in
    u)
        SAFE=false
        ;;
    esac
done

install_apps
# install_tools
# install_dotfiles $SAFE
# install_scripts
# install_plugins
