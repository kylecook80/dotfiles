set nocompatible

set encoding=utf-8
set ruler
set showmatch
set showmode

filetype off
filetype indent on
filetype plugin on

set background=dark
set t_Co=256
set nu
syntax on

set backspace=start,eol,indent
set tabstop=4
set shiftwidth=4
set expandtab
set softtabstop=4

set clipboard=unnamedplus
set mouse=a

